  "scripts": {
    "start": "node ./server/app.js",
    "startdev": "concurrently -k \"ng build --output-path=dist/client --watch --prod\" \"nodemon ./server/app.js\"",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test --watch=false",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build": "ng build --prod --sm=false --aot=true --output-path=dist/client && npm run _server:build",
    "_server:run": "nodemon ./server/app.js",
    "_server:build": "tsc -p ./src/server",
    "postinstall": "npm run build"
  },
